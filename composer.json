{
    "name": "helaplus/laravel-mifos",
    "description": "A laravel package to make it simpler to interact with Mifos API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Le-Yo",
            "email": "leo@helaplus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "helaplus\\laravel-mifos\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Helaplus\\LaravelMifos\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Helaplus\\LaravelMifos\\LaravelMifosServiceProvider"
            ],
            "aliases": {
                "Mifos": "Helaplus\\LaravelMifos\\Facades\\Mifos"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
